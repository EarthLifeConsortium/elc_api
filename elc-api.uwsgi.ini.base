[uwsgi]
socket = :8008
plugins = python3, logfile
die-on-term = true
need-app = true
wsgi-file = swagger_server/app.py
protocol = http
master = True
processes = 2
threads = 2
wsgi-disable-file-wrapper = True
buffer-size = 262144
logto = /var/log/uwsgi/earthlife.err
#log-date = %%F %%T %%Z
logfile-chmod = 644
log-5xx = true
log-4xx = true
disable-logging = true
; stats = 127.0.0.1:9009
; memory-report = True
